is_magician = False
is_expert = False

if is_magician and is_expert:
  print("You are a Master Magician")

elif is_magician and not is_expert:
  print("atleaste you're getting there")

elif is_expert and not is_magician:
  print("You need a magic powers")

elif not is_expert and not is_magician:
  print("Please try to learn atleast one")
